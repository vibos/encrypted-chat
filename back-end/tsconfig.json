{
  "extends": "ts-node/node16/tsconfig.json",
  "ts-node": {
    "transpileOnly": false,
    "files": true,
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2015",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "lib": ["es2020", "dom"],
    "outDir": "./dist"
  }
}